{
  "name": "backendless",
  "version": "4.0.2-beta",
  "description": "Backendless JavaScript SDK for Node.js and the browser",
  "main": "./dist/backendless.js",
  "scripts": {
    "test:tsc": "tsc ./test/tsd.ts",
    "test": "npm run test:e2e",
    "test:e2e": "npm run test:e2e:node",
    "test:e2e:node": "TEST_ENV=node mocha --compilers js:babel-core/register test/e2e/specs",
    "test:e2e:browser": "karma start test/e2e/karma.conf.js",
    "clean": "rimraf dist",
    "copy": "copyup src/* dist",
    "minimize": "uglifyjs dist/backendless.js --mangle --source-map dist/backendless.min.map -o dist/backendless.min.js",
    "build": "npm run clean && npm run copy && npm run minimize",
    "prepublish": "npm run build"
  },
  "files": [
    "dist",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Backendless/JS-SDK"
  },
  "bugs": {
    "url": "http://support.backendless.com",
    "email": "info@backendless.com"
  },
  "keywords": [
    "cloud",
    "mobile",
    "api",
    "baas",
    "backendless.com"
  ],
  "author": "info@backendless.com",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babelify": "^7.3.0",
    "backendless-console-sdk": "^1.0.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "copyfiles": "^1.1.0",
    "eslint": "^3.12.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.8.0"
  }
}
