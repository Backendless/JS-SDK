{
  "name": "backendless",
  "version": "4.0.11",
  "description": "Backendless JavaScript SDK for Node.js and the browser",
  "browser": "dist/backendless.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "backendless.d.ts"
  ],
  "typings": "backendless.d.ts",
  "scripts": {
    "dev": "watch 'npm run build:commonjs' ./src",
    "clean": "rimraf lib dist es",
    "lint": "eslint src --fix",
    "check": "npm run lint && npm run test",
    "test": "npm run test:tsc && npm run test:e2e",
    "test:tsc": "tsc ./test/tsd.ts",
    "test:e2e": "npm run test:e2e:node",
    "test:e2e:node": "TEST_ENV=node mocha --compilers js:babel-core/register test/e2e/specs",
    "test:e2e:browser": "karma start test/e2e/karma.conf.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/backendless.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/backendless.min.js",
    "copy": "copyup src/* dist",
    "bannerize": "bannerize ./dist/*.js --banner=./banner.ejs",
    "prepublish": "npm run clean && npm run copy && npm run build && npm run bannerize"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Backendless/JS-SDK"
  },
  "bugs": {
    "url": "http://support.backendless.com",
    "email": "info@backendless.com"
  },
  "keywords": [
    "cloud",
    "mobile",
    "api",
    "baas",
    "backendless.com"
  ],
  "author": "info@backendless.com",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.17.0",
    "babelify": "^7.3.0",
    "backendless-console-sdk": "^2.0.13",
    "bannerize": "^1.1.2",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "copyfiles": "^1.1.0",
    "cross-env": "^5.0.5",
    "eslint": "^3.12.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3",
    "uglify-js": "^3.0.15",
    "watch": "^1.0.2",
    "watchify": "^3.8.0",
    "webpack": "^3.5.5"
  },
  "dependencies": {
  }
}
